    <table border="1">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Teléfono</th>
                <th>Email</th>
                <th>Fecha de visita</th>
                <th>Cantidad de personas</th>
            </tr>
        </thead>
        <tbody>
           {{#each carga}}
           <tr>
            <td>{{this.nombre}}</td>
            <td>{{this.numtel}}</td>
            <td>{{this.email}}</td>
            <td>{{this.fechaVisita}}</td>
            <td>{{this.cantidadPer}}</td>
           </tr> 
           <script>
             console.log({{this.email}} {{this.numtel}})
